extends layout

block content
  h1= title
  p Welcome to #{title}, this is #{title}
  
  <form method="POST" action="/calc">
  input(type="text" id="var1" name="var1")
  input(type="text" id="var2" name="var2")

  <select name="aritmatika" id="aritmatika">
  option(value="0") Pilih Aritmatika
  option(value="1") +
  option(value="2") -
  option(value="3") *
  option(value="4") /
  </select>

  <input id="calc-set" type="submit" value="SET"/>
  </form>
  
  h2 Hasil :
  p= result